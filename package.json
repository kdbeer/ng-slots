{
  "name": "ng-slot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --prod --aot --build-optimizer -c develop",
    "build-staging": "ng build --prod --aot --build-optimizer -c staging",
    "build-prod": "ng build --prod --aot --build-optimizer -c production",
    "build-aws": "npm run build-prod && aws s3 cp ./dist s3://ng-slots/ --recursive --acl public-read --region ap-southeast-1",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run ng-slot:serve-ssr",
    "serve:ssr": "node dist/ng-slot/server/main.js",
    "build:ssr": "ng build --prod && ng run ng-slot:server:production",
    "prerender": "ng run ng-slot:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.2",
    "@angular/common": "~10.2.2",
    "@angular/compiler": "~10.2.2",
    "@angular/core": "~10.2.2",
    "@angular/forms": "~10.2.2",
    "@angular/platform-browser": "~10.2.2",
    "@angular/platform-browser-dynamic": "~10.2.2",
    "@angular/platform-server": "~10.2.2",
    "@angular/router": "~10.2.2",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/entity": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "@nguniversal/express-engine": "^10.1.0",
    "core-js": "^3.6.5",
    "express": "^4.15.2",
    "rxjs": "~6.6.3",
    "rxjs-compat": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.2",
    "@angular/language-service": "~10.2.2",
    "@ngrx/schematics": "^10.0.1",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.1",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
